{
  "name": "riselms/sarus-sdk-php",
  "homepage": "https://sarus.io",
  "description": "Sarus PHP SDK",
  "keywords": ["sarus", "riselms", "sdk", "php"],
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Sarus team",
      "homepage": "http://buzzi.io"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^6.3",
    "psr/http-message": "^1.0",
    "vlucas/phpdotenv": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "Sarus\\": "src/Sarus/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sarus\\": "test/Sarus/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1"
  }
}
