{
  "name": "sarus/sarus-sdk-php",
  "homepage": "https://sarus.io",
  "description": "Sarus PHP SDK",
  "keywords": ["sarus", "sdk", "php"],
  "type": "library",
  "license": "proprietary",
  "authors": [
    {
      "name": "Sarus team",
      "homepage": "https://sarus.io"
    }
  ],
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "^6.3",
    "psr/http-message": "^1.0",
    "psr/log": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Sarus\\": "src/Sarus/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Unit\\": "test/Unit/",
      "Test\\Functional\\": "test/Functional/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "5.*",
    "vlucas/phpdotenv": "^2.4"
  },
  "config": {
    "bin-dir": "bin"
  }
}
